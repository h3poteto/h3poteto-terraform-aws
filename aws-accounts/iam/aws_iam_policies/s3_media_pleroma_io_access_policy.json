{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowUserToS3ReadWriteAccess",
      "Effect": "Allow",
      "Action": [
        "s3:ListBucket",
        "s3:GetObject",
        "s3:GetObjectAcl",
        "s3:DeleteObject",
        "s3:PutObject",
        "s3:PutObjectAcl"
      ],
      "Resource": [
        "arn:aws:s3:::media.pleroma.io/*",
        "arn:aws:s3:::media.pleroma.io"
      ]
    }
  ]
}
