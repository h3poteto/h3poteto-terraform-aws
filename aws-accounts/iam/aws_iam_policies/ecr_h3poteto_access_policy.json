{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowUsersToLoginECR",
      "Effect": "Allow",
      "Action": [
        "ecr:GetAuthorizationToken"
      ],
      "Resource": "*"
    },
    {
